{
  "name": "primordyn",
  "version": "0.1.0",
  "description": "Auto-documentation engine for AI-assisted development - indexes codebases and serves intelligent context",
  "main": "dist/index.js",
  "bin": {
    "primordyn": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "claude-code",
    "ai-assistant",
    "code-intelligence",
    "code-context",
    "documentation",
    "indexing",
    "static-analysis",
    "cli",
    "llm",
    "copilot",
    "context-engineering"
  ],
  "author": "Parley Hatch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatch25/Primordyn.git"
  },
  "bugs": {
    "url": "https://github.com/phatch25/Primordyn/issues"
  },
  "homepage": "https://github.com/phatch25/Primordyn#readme",
  "dependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "fuse.js": "^7.1.0",
    "ignore": "^7.0.5",
    "js-tiktoken": "^1.0.21",
    "ora": "^8.2.0",
    "typescript": "^5.9.2",
    "web-tree-sitter": "^0.25.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/babel__traverse": "^7.28.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.4",
    "typescript-eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "glob": "^10.4.5",
    "inflight": "npm:lru-cache@^10.0.0"
  }
}
